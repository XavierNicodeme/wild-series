{% extends 'base.html.twig' %}

{% block title %}All Categories{% endblock %}

{% block body %}
    <ul>
        {% for category in categories %}
            <li>
                <a href="{{ path("category_show", { categoryName: category.name }) }}">{{ category.name }}</a>
            </li>
        {% endfor %}
    </ul>
    <a href="{{ path('app_index') }}">Home</a>
{% endblock %}
